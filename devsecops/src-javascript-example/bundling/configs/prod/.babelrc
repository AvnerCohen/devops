{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 Chrome versions",
            "last 2 Edge versions",
            "last 2 Safari versions",
            "last 2 Firefox versions",
            "IE >= 11"
          ]
        }
      }
    ],
    [
      "minify",
      {
        "removeConsole": true,
        "removeDebugger": true
      }
    ]
  ],
  "plugins": [
    "transform-object-assign"
  ]
}
